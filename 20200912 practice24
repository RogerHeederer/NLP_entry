      "source": [
        "normalized_text = []\n",
        "for string in sent_text:\n",
        "  tokens = re.sub(r\"[^a-z0-9]+\", \" \", string.lower())\n",
        "  normalized_text.append(tokens)\n",
        "  #각 문장에 대해 구두점 제거. 대문자->소문자 변환\n",
        "\n",
        "result = [word_tokenize(sentence) for sentence in normalized_text] #토크나이징"
      ],
      "execution_count": 12,
      "outputs": []
