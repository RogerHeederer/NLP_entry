    {
      "cell_type": "code",
      "metadata": {
        "id": "Jj9Svz26d71c",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "#형태소 분석기 OKT를 사용하여 토큰화 작업\n",
        "okt = Okt()\n",
        "tokenized_data = []\n",
        "for sentence in train_data['document']:\n",
        "  temp_X = okt.morphs(sentence, stem=True) #토큰화\n",
        "  temp_X = [word for word in temp_X if not word in stopwords] #불용어 제거\n",
        "  tokenized_data.append(temp_X)"
      ],
      "execution_count": 39,
      "outputs": []
    },
